{
    "gitlens.autolinks": [
        {
            "prefix": "#",
            "url": "$GIT_HOME_URL/issues/<num>"
        }
    ],
    "autoDocstring.quoteStyle": "'''",
    "autoDocstring.startOnNewLine": true,
    "autoDocstring.docstringFormat": "google-notypes",

    "python.envFile": "${workspaceFolder}/.env",

    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
        "WORKDIR": "${workspaceFolder}"
    },
    "terminal.integrated.profiles.linux": {
        "local": {
            "path": "bash",
            "icon": "terminal",
            "args": [
                "-c",
                "if [ -e .env ]; then export `cat .env`; fi; if [ -e .vscode/.env ]; then export `cat .vscode/.env`; fi; bash"
            ]
        }
    },
    "terminal.integrated.defaultProfile.linux": "local",
    "cSpell.words": [
        "asgi",
        "asgiref",
        "Asyncio",
        "closefd",
        "Comfortaa",
        "crossorigin",
        "DEVPATH",
        "glightbox",
        "hypercorn",
        "inlinehilite",
        "ipykernel",
        "ipynb",
        "isabstractmethod",
        "jupyterlab",
        "layouter",
        "linenums",
        "LITERA",
        "metageneration",
        "metatag",
        "metatags",
        "mkdocs",
        "mkdocstrings",
        "multicloud",
        "multipage",
        "omnicloud",
        "Plotly",
        "pylint",
        "pyyaml",
        "qualname",
        "setuptools",
        "skipkeys",
        "SPACELAB",
        "subpackages",
        "supercloud",
        "wght",
        "wsgi"
    ],
    "cSpell.ignorePaths": [
        "poetry.lock",
        ".gitignore",
        ".git/objects",
        ".vscode/**",
        ".github/**"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.analysis.ignore": [
        "**/*.ipynb"
    ]

}